# 智谱AI API配置
ZHIPUAI_API_KEY=your_zhipuai_api_key
ZHIPUAI_API_URL=https://open.bigmodel.cn/api/paas/v4/chat/completions
ZHIPUAI_MODEL=glm-4
ZHIPUAI_TEMPERATURE=0.7
ZHIPUAI_TOP_P=0.8

# DeepSeek API配置
DEEPSEEK_API_KEY=your_deepseek_api_key
DEEPSEEK_API_URL=https://api.deepseek.com/v1/chat/completions
DEEPSEEK_MODEL=deepseek-chat
DEEPSEEK_TEMPERATURE=0.7
DEEPSEEK_TOP_P=0.8

# SearXNG配置
SEARXNG_API_HOST=https://your-searxng-instance.com
SEARXNG_DEFAULT_LANGUAGE=auto
SEARXNG_DEFAULT_SAFESEARCH=0
SEARXNG_DEFAULT_ENGINES=bing,baidu,360search,quark,sogou
SEARXNG_DEFAULT_TIME_RANGE=month

# Bocha AI配置
BOCHAAI_API_KEY=your_bochaai_api_key
BOCHAAI_DEFAULT_FRESHNESS=oneMonth
# 注意：所有搜索引擎和模型接口的设置都在.env文件中配置
